    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-standard": "^17.0.0",
